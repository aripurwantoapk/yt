sudo apt update
wget https://raw.githubusercontent.com/aripurwantoapk/yt/main/file/googlechrome.deb
wget https://raw.githubusercontent.com/aripurwantoapk/yt/main/file/ytviewer.zip
sudo apt install -y ./googlechrome.deb
sudo apt install unzip
unzip ytviewer.zip
cd YouTube-Viewer
sudo apt install -y python3-pip
pip3 install "setuptools<59"
pip3 install -r requirements.txt
rm -rf urls.txt search.txt
